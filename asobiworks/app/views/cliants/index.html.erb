<p id="notice"><%= notice %></p>

<h1>Listing Cliants</h1>


<div>
	<% @cliants.each do |cliant| %>
	<%= cliant.title %>
	<div class="form-group">
	<%= cliant.area %>
	<div class="form-group">

	<%= cliant.name %>
	<div class="form-group">
	<label class="control-label col-sm-2" for="email">メールアドレス</label>
		<%= cliant.email %>
	<label class="control-label col-sm-2" for="tel">TEL（任意）</label>
      <div class="col-sm-10">

	<div class="form-group">
		<label class="control-label col-sm-2" for="pass">最少催行人数</label>
		<div class="col-sm-10">
			<%= cliant.smallnumber %>
		</div>
	</div>
	<div class="form-group">
		<label class="control-label col-sm-2" for="contents">プロジェクトの概要</label>
		<div class="col-sm-10">
			<%= cliant.concept %>
	</div>
	<p><%= link_to 'Show', cliant %></p>
	<p><%= link_to 'Edit', edit_cliant_path(cliant) %></p>
	<p><%= link_to 'Destroy', cliant, method: :delete, data: { confirm: 'Are you sure?' } %></p>
</div>

<br>

<%= link_to 'New Cliant', new_cliant_path %>
