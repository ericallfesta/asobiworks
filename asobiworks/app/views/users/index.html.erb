<p id="notice"><%= notice %></p>

<h1>Listing Users</h1>

<div>
  <% @users.each do |user| %>
     <p>名前</p>
     <p><%= user.name %></p>
     <p>年齢</p>
     <p><%= user.age %></p>
     <p></p>
     <p><%= link_to 'Show', user %></p>
     <p><%= link_to 'Edit', edit_user_path(user) %></p>
     <p><%= link_to 'Destroy', user, method: :delete, data: { confirm: 'Are you sure?' } %></p>
  <% end %>
</div>


<%= link_to 'New User', new_user_path %>
